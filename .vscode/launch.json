    "version": "0.2.0",
    "configurations": [

        {
            "name": "Python: run-program",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/tests/debug.py",
            "subProcess": true,
            // "args" : ["--filename", "tests/data/tacview-test3.txt", "--bulk", "--iters", "50"]
        },
        {
            "name": "Python: Current File (Integrated Terminal)",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },

        {
            "name": "Python: Module",
            "type": "python",
            "request": "launch",
            "module": "tacview_client",
            "console": "integratedTerminal"
        },
    ]
}